<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Register Suite">
    <listeners>
        <listener class-name="Listeners.IInvokedmethod"/>
        <listener class-name="Listeners.ITestmethod"/>
    </listeners>
    <test name="Register test on chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="Tests.TC01RegisterPage"/>
        </classes>
    </test>
    <test name="Register test on edge">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="Tests.TC01RegisterPage"/>
        </classes>
    </test>
</suite>